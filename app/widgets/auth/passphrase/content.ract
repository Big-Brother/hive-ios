    <label class="_label">{{translate("Enter Passphrase")}}:
      <input id="passphraseField" class="_input" type="text" placeholder="12-word passphrase" autocorrect="on" autocapitalize="off" value="{{passphrase}}" on-enter="open-wallet-with-passphrase" />
    </label>
  </div>
</div>
